<h3 style="margin-left: 44%;margin-top: 15px;">Categories</h3>
<table style="margin-left: 35%;">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
  <% @categories.each do |category| %>
    <tr>
      <td><%= category.name %></td>
      <td><%= category.description %></td>
    </tr>
  <% end %>
</table>